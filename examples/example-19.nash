#!/bin/nash

# Break and Continue

var data = [1, 2, 3, 4, 5];

for value in data {
  if value == 3 {
    continue;
  };

  out(value.fmt());
};

out("Done for loop!");
out("");

var mut index = 0;
while true {
  index = index + 1;

  if index > 3 {
    break;
  };

  out(index.fmt());
};

out("Done while loop!");
